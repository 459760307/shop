/* pages/discover/discover.wxss */
navigator{
  box-sizing: border-box;
}
page {
  background-color: #f6f6f6;
}

.bdr-bottom {
  border-bottom: 1px solid #f5f5f5;
}

.floor-tit-img {
  height: 70rpx;
  margin-top: 10rpx;
}

.floor-container {
  overflow: hidden;
}

.flsPit {
  height: 200rpx;
  background-color: #f5f5f5;
}

.floor-graphic-item {
  width: 100%;
  height: 200rpx;
  background: #fff;
  overflow: hidden;
}

.floor-graphic-item .bdr-r {
  border-right: 1px solid #f5f5f5;
}

.graphic-col02-sm {
  width: 50%;
  position: relative;
  height: 100%;
  float: left;
}

.real-show {
  overflow: hidden;
  padding: 13rpx 0 10rpx;
}

.real-show .show-img {
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: absolute;
  left: 0;
  top: 0;
}

.graphic-text {
  float: left;
  width: 202rpx;
  padding: 0 18rpx 0 16rpx;
  overflow: hidden;
}

.graphic-tit {
  font-size: 32rpx;
  line-height: 44rpx;
  color: #222;
  display: inline-block;
  max-width: 100%;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.graphic-wz {
  font-size: 24rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  display: box;
}

.graphic-col02-sm .graphic-img {
  width: 160rpx;
  height: 160rpx;
  margin: 17rpx 10rpx 0 0;
  float: right;
  background: #f5f5f5;
}

.graphic-img .img {
  width: 160rpx;
  height: 160rpx;
}
