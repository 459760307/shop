/* pages/cart/cart.wxss */

.wrap {
  min-height: 300rpx;
  padding: 20rpx;
}

.select-all {
  padding-bottom: 20rpx;
  height: 36rpx;
  font-size: 24rpx;
  color: #24292e;
}

.checkbox {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  border: 1px solid #666;
}

.checkbox.active {
  border: none;
  background: #e4393c url(data:image/png;base64,) no-repeat center center/32rpx auto;
}

.ul {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.li {
  border-top: 1rpx solid #e5e5e5;
  padding: 20rpx 0;
  height: 200rpx;
}

.li:first-child {
  border: none;
}

.check {
  float: left;
}

.pic {
  float: left;
  width: 150rpx;
  height: 150rpx;
  background: no-repeat center/cover;
}

.desc {
  float: left;
}

.name {
  font-size: 28rpx;
  font-weight: 700;
  color: #333;
}

.attr {
  font-size: 28rpx;
  color: #666;
}

.price {
  font-size: 32rpx;
  color: #e4393c;
}

.minus {
  font-size: 32rpx;
}

.number {
  padding: 0 30rpx;
}

.plus {
  font-size: 32rpx;
}

.delete {
  float: left;
}

/**/

.fixed {
  overflow: hidden;
  z-index: 100000;
  position: fixed;
  width: 100%;
  bottom: 0;
  left: 0;
  right: 0;
  margin: 0 auto;
  background-color: hsla(0, 0%, 100%, 0.95);
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100rpx;
}

.cart {
  padding-left: 20rpx;
  text-align: center;
  line-height: 100rpx;
  height: 100%;
  font-size: 24rpx;
  width: 15%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
}

.add {
  text-align: center;
  line-height: 100rpx;
  height: 100%;
  font-size: 24rpx;
  /* background: #ff9600; *//* color:#fff; */
  width: 35%;
}

.buy {
  text-align: center;
  line-height: 100rpx;
  height: 100%;
  font-size: 24rpx;
  background: #e4393c;
  color: #fff;
  width: 35%;
}

.fixed .icon {
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  width: 26rpx;
  height: 26rpx;
  border-radius: 50%;
  color: white;
  background: #e4393c;
  line-height: 26rpx;
  text-align: center;
  font-size: 14rpx;
}
